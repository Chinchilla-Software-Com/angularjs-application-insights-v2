{
    "compilerOptions": {
      "target": "es5",
      "sourceMap": false,
      "declaration": false,
      "outDir": "./dist",
      "typeRoots": [
        "./node_modules/@types"
      ],
      "preserveConstEnums": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "removeComments": true
    },
    "include": [
      "src/**/*",
      "index.ts"
  ],
    
    "exclude": [
      "node_modules",
      "dist"
    ]
  }